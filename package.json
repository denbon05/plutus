    "body-parser": "^1.19.1",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.19.3",
    "currency-list": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.15",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nuxt": "^2.15.8",
    "nuxt-buefy": "^0.4.13",
    "nuxt-rollbar-module": "^1.1.0",
    "pg": "^8.7.1",
    "vue": "^2.6.14",
