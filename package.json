{
  "name": "plutus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "pre-build": "rm -fr ./dist ./.nuxt",
    "build-db": "knex migrate:latest",
    "build": "npm run pre-build & nuxt build",
    "build-all": "npm run build & npm run build-db",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "npx prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "jest"
  },
  "pre-commit": [
    "test"
  ],
